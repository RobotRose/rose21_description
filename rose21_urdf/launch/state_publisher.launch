<?xml version="1.0"?>
<launch>
    <!-- <param name="/rose/robot_description" command="cat $(find rose21_urdf)/urdf/rose21.xml" /> -->
    <param name="/rose/robot_description" command="$(find xacro)/xacro.py '$(find rose21_urdf)/rose21.xacro'" />

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="rob_st_pub" >
        <remap from="robot_description" to="rose/robot_description" />
        <!-- <remap from="joint_states" to="different_joint_states" /> -->
    </node>

    <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_publisher">
        <remap from="robot_description" to="rose/robot_description" />
        <rosparam>
            source_list: ['/lift_controller/lift/joint_states', '/neck/jointstates', 'mico_arm/joint_states']
            use_gui: false
       </rosparam>
    </node>
   
    <include file="$(find rose21_urdf)/launch/neck_dynamixels.launch"/>
   
    <node pkg="tf" type="static_transform_publisher" name="prefix_camera_rose" args="0 0 0 0 0 0 /camera_rgb_optical_frame /rose/camera_rgb_optical_frame 10"/>
</launch>
