  </inertial>
  <visual>
    <origin xyz="0 0 0.24" rpy="0 0 0" />
    <geometry>
      <box size="0.29 0.24 0.48" />
    </geometry>
    <material name="ral9010">
      <color rgba=".8 .8 .8 1" />
    </material>
  </visual>
  <collision>
    <origin xyz="0 0 0.24" rpy="0 0 0" />
    <geometry>
      <box size="0.29 0.24 0.48" />
    </geometry>
    <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
  </collision>
</link>

 <joint name="lift_bottom" type="fixed">
    <parent link="base_link"/>
    <child link="lift_link"/>
  </joint>
</robot>
