    <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
    <origin/>
  </inertial>
  <visual>
    <origin xyz="0 0 0.135" rpy="0 0 0" />
    <geometry>
      <box size="0.8 0.6 0.27" />
    </geometry>
    <material name="ral9010">
      <color rgba=".8 .8 .8 1" />
    </material>
  </visual>
  <collision>
    <origin xyz="0 0 0.135" rpy="0 0 0" />
    <geometry>
      <box size="0.8 0.6 0.27" />
